# API Configuration
REACT_APP_API_URL=http://localhost:3001
REACT_APP_API_VERSION=v1

# WebSocket Configuration
REACT_APP_WS_URL=ws://localhost:3001/notifications
REACT_APP_WS_RECONNECT_INTERVAL=5000
REACT_APP_WS_MAX_RECONNECT_ATTEMPTS=5

# Application Configuration
REACT_APP_APP_NAME=Online Quotation Tool
REACT_APP_APP_VERSION=1.0.0
REACT_APP_APP_ENVIRONMENT=development

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_DEBUG_MODE=true
REACT_APP_ENABLE_MOCK_DATA=false

# External Services
REACT_APP_GOOGLE_ANALYTICS_ID=
REACT_APP_SENTRY_DSN=

# Development
REACT_APP_LOG_LEVEL=debug
REACT_APP_ENABLE_HOT_RELOAD=true
